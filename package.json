{
  "name": "service-scaffolding",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx jest --coverage --detectOpenHandles",
    "prebuild": "rimraf ./dist",
    "test-only": "npx jest --detectOpenHandles -- ",
    "build": "swc src -d dist",
    "start": "npm run build && node dist/index.js",
    "build-dev": "npm run build && cross-env NODE_ENV=development node --inspect dist/index.js",
    "dev": "nodemon --ext ts --exec \"npm run build-dev\"",
    "compile-schemas": "json2ts -i schemas -o types",
    "migrate:up:system": "npx knex migrate:latest --migrations-directory ./migrations/system --env=development"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "module": "true",
  "dependencies": {
    "@fastify/autoload": "^4.0.1",
    "@fastify/cors": "7.0.0",
    "@fastify/error": "2.0.0",
    "@fastify/jwt": "^5.0.1",
    "ajv": "^8.11.0",
    "axios": "^0.27.2",
    "bluebird": "^3.7.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "fastify": "^3.29.0",
    "fastify-error": "^1.2.0",
    "fastify-plugin": "^3.0.1",
    "fastify-qs": "3.0.0",
    "json-schema-to-typescript": "^10.1.5",
    "knex": "^2.0.0",
    "pg": "^8.7.3",
    "pg-promise": "^10.11.1",
    "prettier": "^2.6.2",
    "short-uuid": "^4.2.0",
    "winston": "^3.4.0",
    "winston-elasticsearch": "^0.16.1",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.186",
    "@swc/register": "^0.1.10",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "jsc": {
    "parser": {
      "syntax": "ecmascript",
      "dynamicImport": true
    }
  }
}
